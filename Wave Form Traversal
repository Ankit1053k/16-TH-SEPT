#include <iostream> 
using namespace std; 
int main() { 
    int N, M; 
    cin >> N >> M; 
 
    int arr[100][100];  // as per constraints 
    for (int i = 0; i < N; i++) { 
        for (int j = 0; j < M; j++) { 
            cin >> arr[i][j]; 
        } 
    } 
 
    // Wave Traversal 
    for (int col = 0; col < M; col++) { 
        if (col % 2 == 0) { 
            // top → bottom 
            for (int row = 0; row < N; row++) { 
                cout << arr[row][col] << " "; 
            } 
        } else { 
            // bottom → top 
            for (int row = N - 1; row >= 0; row--) { 
                cout << arr[row][col] << " "; 
            } 
        } 
    } 
 
    return 0; 
}
