#include <iostream> 
using namespace std; 
 
int main() { 
    int N; 
    cin >> N; 
 
    int arr[100][100];  // as per constraints 
    for (int i = 0; i < N; i++) { 
        for (int j = 0; j < N; j++) { 
            cin >> arr[i][j]; 
        } 
    } 
 
    // Step 1: Transpose 
    for (int i = 0; i < N; i++) { 
        for (int j = i + 1; j < N; j++) { 
            int temp = arr[i][j]; 
            arr[i][j] = arr[j][i]; 
            arr[j][i] = temp; 
        } 
    } 
 
    // Step 2: Reverse each row 
    for (int i = 0; i < N; i++) { 
        int left = 0, right = N - 1; 
        while (left < right) { 
            int temp = arr[i][left]; 
            arr[i][left] = arr[i][right]; 
            arr[i][right] = temp; 
            left++; 
            right--; 
        } 
    } 
 
    // Print rotated matrix 
    for (int i = 0; i < N; i++) { 
        for (int j = 0; j < N; j++) { 
            cout << arr[i][j] << " "; 
        } 
        cout << endl; 
    } 
 
    return 0; 
}
